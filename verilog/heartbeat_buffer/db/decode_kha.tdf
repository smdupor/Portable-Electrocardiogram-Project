--synthesis_resources = lut 4 
SUBDESIGN decode_kha
( 
	data[1..0]	:	input;
	enable	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	enable_wire	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode553w[2..0]	: WIRE;
	w_anode566w[2..0]	: WIRE;
	w_anode574w[2..0]	: WIRE;
	w_anode582w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	enable_wire = enable;
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode582w[2..2], w_anode574w[2..2], w_anode566w[2..2], w_anode553w[2..2]);
	w_anode553w[] = ( (w_anode553w[1..1] & (! data_wire[1..1])), (w_anode553w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode566w[] = ( (w_anode566w[1..1] & (! data_wire[1..1])), (w_anode566w[0..0] & data_wire[0..0]), enable_wire);
	w_anode574w[] = ( (w_anode574w[1..1] & data_wire[1..1]), (w_anode574w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode582w[] = ( (w_anode582w[1..1] & data_wire[1..1]), (w_anode582w[0..0] & data_wire[0..0]), enable_wire);
END;
